proxy-providers:
  daili:
    type: http
    url: {{ .daili_proxy_url }}
    path: proxy-providers/daili.yaml
    interval: 0
    proxy: DIRECT
    exclude-type: hysteria2
    override:
      udp-over-tcp: true
      udp-over-tcp-version: 2

proxies:
  - name: warp
    type: wireguard
    dialer-proxy: dailiproxy
    private-key: {{ .warp_private_key }}
    public-key: {{ .warp_public_key }}
    ip: 172.16.0.2/32
    ipv6: {{ .warp_ipv6 }}
    server: engage.cloudflareclient.com
    port: 2408
    allowed-ips: ['0.0.0.0/0', '::/0']
    udp: true
    mtu: 1280

  - name: vless-us-warp
    type: vless
    server: {{ .vless_server }}
    port: 443
    udp: true
    uuid: {{ .vless_us_uuid }}
    flow: xtls-rprx-vision
    packet-encoding: xudp
    tls: true
    servername: {{ .vless_sni }}
    encryption: {{ .vless_us_enc }}
    reality-opts:
      public-key: {{ .vless_public_key }}
      short-id: {{ .vless_short_id }}
      support-x25519mlkem768: true

  - name: vless-us
    type: vless
    server: {{ .vless_server }}
    port: 8443
    udp: true
    uuid: {{ .vless_jp_uuid }}
    flow: xtls-rprx-vision
    packet-encoding: xudp
    tls: true
    servername: {{ .vless_sni }}
    encryption: {{ .vless_jp_enc }}
    reality-opts:
      public-key: {{ .vless_public_key }}
      short-id: {{ .vless_short_id }}
      support-x25519mlkem768: true


  - name: hy2-us
    server: {{ .vless_server }}
    port: 443
    type: hysteria2
    password: {{ .hy2_passwd }}
    sni: {{ .hy2_sni }}
    fingerprint: {{ .hy2_fingerprint }}
    alpn:
      - h3

proxytest: &proxytest
  url: https://www.gstatic.com/generate_204
  interval: 0
  timeout: 2000
  max-failed-times: 5
  expected-status: 204

