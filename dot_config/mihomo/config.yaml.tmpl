# ä»£ç†é›†åˆ
proxy-providers:
  daili:
    type: http
    url: {{ .daili_proxy_url }}
    path: proxy-providers/daili.yaml
    interval: 0
    proxy: DIRECT
    exclude-type: hysteria2
    override:
      udp-over-tcp: true

  wuxian1:
    type: http
    url: {{ .wuxian1_proxy_url }}
    path: proxy-providers/wuxian1.yaml
    interval: 43200
    proxy: DIRECT
    exclude-type: hysteria2

proxies:
  - name: warp
    type: wireguard
    dialer-proxy: dailiproxy
    private-key: {{ .warp_private_key }}
    public-key: {{ .warp_public_key }}
    ip: 172.16.0.2/32
    ipv6: {{ .warp_ipv6 }}
    server: engage.cloudflareclient.com
    port: 4500
    allowed-ips: ['0.0.0.0/0', '::/0']
    udp: true
    mtu: 1280

  - name: vless-us
    type: vless
    server: {{ .vless_server_ip }}
    port: 7878
    udp: true
    uuid: {{ .vless_us_uuid }}
    flow: xtls-rprx-vision
    packet-encoding: xudp
    tls: true
    servername: {{ .vless_sni }}
    encryption: {{ .vless_us_enc }}
    reality-opts:
      public-key: {{ .vless_public_key }}
      short-id: {{ .vless_short_id }}

  - name: vless-jp
    type: vless
    server: {{ .vless_server_ip }}
    port: 7879
    udp: true
    uuid: {{ .vless_jp_uuid }}
    flow: xtls-rprx-vision
    packet-encoding: xudp
    tls: true
    servername: {{ .vless_sni }}
    encryption: {{ .vless_jp_enc }}
    reality-opts:
      public-key: {{ .vless_public_key }}
      short-id: {{ .vless_short_id }}

