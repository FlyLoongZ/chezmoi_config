# 是否允许局域网
allow-lan: false
# 绑定地址
bind-address: 0.0.0.0
# 混合端口
mixed-port: 7890

# 运行模式
mode: rule
# 日志级别
log-level: warning

# ipv6
ipv6: true

# 进程匹配模式
find-process-mode: strict

# 外部控制 (API)
external-controller: 127.0.0.1:9090
# API 访问密钥
secret: "mihomo123456"
# 外部用户界面
external-ui: zashboard
# 外部用户界面下载地址
external-ui-url: https://github.com/Zephyruso/zashboard/releases/latest/download/dist.zip

profile:
  # 储存 API 对策略组的选择，以供下次启动时使用
  store-selected: true

# 统一延迟
unified-delay: true
# tcp 并发
tcp-concurrent: true

# 全局客户端指纹
global-client-fingerprint: chrome

# GEOIP 数据模式
geodata-mode: true
# 自动更新 GEO
geo-auto-update: true
geo-update-interval: 24
# 自定义 GEO 下载地址
geox-url:
  geoip: https://github.com/MetaCubeX/meta-rules-dat/releases/download/latest/geoip.dat
  geosite: https://github.com/MetaCubeX/meta-rules-dat/releases/download/latest/geosite.dat
  mmdb: https://github.com/MetaCubeX/meta-rules-dat/releases/download/latest/country-lite.mmdb
  asn: https://github.com/MetaCubeX/meta-rules-dat/releases/download/latest/GeoLite2-ASN.mmdb

localdns: &localdns 127.0.0.1
dns:
  # 启用dns模块
  enable: true
  # 启用ipv6 dns解析
  ipv6: true
  enhanced-mode: redir-host
  cache-algorithm: arc
  respect-rules: true
  default-nameserver:
    - *localdns
  proxy-server-nameserver:
    - *localdns
  nameserver:
    - *localdns
  nameserver-policy:
    "geosite:cn":
      - *localdns
    "rule-set:domestic-non_ip":
      - *localdns
    "geosite:!cn":
      - https://1.1.1.1/dns-query
      - https://8.8.8.8/dns-query
    "rule-set:global-non_ip":
      - https://1.1.1.1/dns-query
      - https://8.8.8.8/dns-query

proxytest: &proxytest
  url: https://cp.cloudflare.com
  interval: 0
  timeout: 2000
  max-failed-times: 5
  lazy: true
  expected-status: 204

# 代理集合
proxy-providers:
  daili:
    type: http
    url: {{ .daili_proxy_url }}
    path: proxy-providers/daili.yaml
    interval: 43200
    proxy: DIRECT
    exclude-type: hysteria2
    override:
      udp-over-tcp: true
      udp-over-tcp-version: 2

  wuxian1:
    type: http
    url: {{ .wuxian1_proxy_url }}
    path: proxy-providers/wuxian1.yaml
    interval: 43200
    proxy: DIRECT
    exclude-type: hysteria2
    override:
      additional-suffix: w1

  wuxian2:
    type: http
    url: {{ .wuxian2_proxy_url }}
    path: proxy-providers/wuxian2.yaml
    interval: 43200
    proxy: DIRECT
    exclude-type: hysteria2
    override:
      additional-suffix: w2

proxies:
  - name: warp
    type: wireguard
    dialer-proxy: dailiproxy
    private-key: {{ .warp_private_key }}
    public-key: {{ .warp_public_key }}
    ip: 172.16.0.2
    ipv6: 2606:4700:110:8c9e:210a:1e69:57ff:bed
    server: engage.cloudflareclient.com
    port: 2408
    allowed-ips: ['0.0.0.0/0', '::/0']
    reserved: CxiT
    udp: true
    mtu: 1280

# 代理组
proxy-groups:
  - name: dailiproxy
    type: select
    proxies:
      - 美国-daili
      - 日本-daili
      - 新加坡-daili
      - 香港-daili
      - 台湾-daili
      - 其它地区-daili
      - 全部节点-daili
      - DIRECT

  - name: warproxy
    type: select
    proxies:
      - warp
      - dailiproxy

  - name: wuxianproxy
    type: select
    proxies:
      - 美国-wuxian
      - 日本-wuxian
      - 新加坡-wuxian
      - 香港-wuxian
      - 台湾-wuxian
      - 其它地区-wuxian
      - 全部节点-wuxian
      - dailiproxy

  - name: 美国-daili
    type: select
    filter: "(?i)美国|us|unitedstates|united states"
    <<: *proxytest
    use:
      - daili

  - name: 美国-wuxian
    type: url-test
    filter: "(?i)美国|us|unitedstates|united states"
    url: https://www.google.com/generate_204
    interval: 7200
    timeout: 2000
    max-failed-times: 5
    lazy: false
    tolerance: 200
    use:
      - wuxian1
      - wuxian2

  - name: 日本-daili
    type: select
    filter: "(?i)日本|jp|japan"
    <<: *proxytest
    use:
     - daili

  - name: 日本-wuxian
    type: select
    filter: "(?i)日本|jp|japan"
    <<: *proxytest
    use:
     - wuxian1
     - wuxian2

  - name: 新加坡-daili
    type: select
    filter: "(?i)(新加坡|sg|singapore)"
    <<: *proxytest
    use:
      - daili

  - name: 新加坡-wuxian
    type: select
    filter: "(?i)(新加坡|sg|singapore)"
    <<: *proxytest
    use:
      - wuxian1
      - wuxian2

  - name: 香港-daili
    type: select
    filter: "(?i)香港|hk|hongkong|hong kong"
    <<: *proxytest
    use:
      - daili

  - name: 香港-wuxian
    type: select
    filter: "(?i)香港|hk|hongkong|hong kong"
    <<: *proxytest
    use:
      - wuxian1
      - wuxian2

  - name: 台湾-daili
    type: select
    filter: "(?i)台湾|tw|taiwan"
    <<: *proxytest
    use:
      - daili

  - name: 台湾-wuxian
    type: select
    filter: "(?i)台湾|tw|taiwan"
    <<: *proxytest
    use:
      - wuxian1
      - wuxian2

  - name: 其它地区-daili
    type: select
    filter: "(?i)^(?!.*(?:🇭🇰|🇯🇵|🇺🇸|🇸🇬|🇨🇳|香港|hk|hongkong|台湾|tw|taiwan|日本|jp|japan|新加坡|sg|singapore|美国|us|unitedstates)).*"
    <<: *proxytest
    use:
      - daili

  - name: 其它地区-wuxian
    type: select
    filter: "(?i)^(?!.*(?:🇭🇰|🇯🇵|🇺🇸|🇸🇬|🇨🇳|香港|hk|hongkong|台湾|tw|taiwan|日本|jp|japan|新加坡|sg|singapore|美国|us|unitedstates)).*"
    <<: *proxytest
    use:
      - wuxian1
      - wuxian2

  - name: 全部节点-daili
    type: select

    use:
     - daili

  - name: 全部节点-wuxian
    type: select
    use:
     - wuxian1
     - wuxian2

ruleset_domain: &ruleset_domain {type: http, interval: 43200, behavior: domain, format: text, proxy: dailiproxy}
ruleset_ipcidr: &ruleset_ipcidr {type: http, interval: 43200, behavior: ipcidr, format: text, proxy: dailiproxy}
ruleset_classical: &ruleset_classical {type: http, interval: 43200, behavior: classical, format: text, proxy: dailiproxy}

